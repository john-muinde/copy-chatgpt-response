{
    "manifest_version": 2,
    "version": "1.0",
    "name": "copy code",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "main.js"
            ],
            "run_at": "document_idle",
            "permissions": [
                "webNavigation",
                "tabs"
            ]
        }
    ]
}